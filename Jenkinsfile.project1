pipeline {
    agent any
    tools{
        nodejs 'Nodejs'
    }
    stages{
        stage('Teste'){
            steps {
                dir('project1'){
                    sh 'npm install'
                    sh 'npm run test'
                }
            }
        }
        stage('Build'){
            steps {
                dir('project1'){
                    sh 'npm run build'
                }
            }
        }
    }
}